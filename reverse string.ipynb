   "source": [
    "#Write a Python program that accepts a word from the user and reverse it.\n",
    "a=str(input(\"enter the name: \"))\n",
    "print(\"the original string is : \",a)\n",
    "\n",
    "jj=\"\"\n",
    "count=len(a)\n",
    "\n",
    "while count>0:\n",
    "    jj+=a[count-1]\n",
    "    count=count-1\n",
    "print(\"the reversed  string is : \",jj)"
   ]
  }
